                      td.text-right.font-weight-bold -
                      td.text-right.font-weight-bold
                        span.badge.badge-danger unknown lib
                    // Invoice item 3
                    tr.border-bottom
                      td
                        .font-weight-bold Spring JDBC
                        .small.text-muted.d-none.d-md-block Spring JDBC
                      td.text-right.font-weight-bold v5.3.3
                      td.text-right.font-weight-bold v5.3.3
                      td.text-right.font-weight-bold
                        span.badge.badge-success All Good
                    // Invoice subtotal
                    tr
                      td.text-right.pb-0(colspan='3')
                        .text-uppercase.small.font-weight-700.text-muted Warning:
                      td.text-right.pb-0
                        .h5.mb-0.font-weight-700.text-warning 1
                    // Invoice tax column
                    tr
                      td.text-right.pb-0(colspan='3')
                        .text-uppercase.small.font-weight-700.text-muted Alert:
                      td.text-right.pb-0
                        .h5.mb-0.font-weight-700.text-danger 1
