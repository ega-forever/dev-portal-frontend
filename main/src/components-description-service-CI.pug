extends ./components/layout

block content
  main
    header.page-header.page-header-compact.page-header-light.border-bottom.bg-white.mb-4
      .container-fluid
        .page-header-content
          .row.align-items-center.justify-content-between.pt-3
            .col-auto.mb-3
              h1.page-header-title
                .page-header-icon
                  i(data-feather='info')
                | Service info
    // Main page content
    .container.mt-4
      // Account page navigation
      nav.nav.nav-borders
        a.nav-link(href='components-description-service.html') Info
        a.nav-link(href='components-description-service-library-analyzer.html') Library Analyzer
        a.nav-link.active.ml-0(href='components-description-service-CI.html') CI
        a.nav-link(href='account-notifications.html') Trail
      hr.mt-0.mb-4
      .row
        .col-xl-4
          // Profile picture card
          .card
            .card-header-actions
              .card-header
                | Package description
                a(href="#")
                  i(data-feather='edit')

            .card-body.text-center
